{
  "theme": "Default",
  "mcpServers": {
    "sequentialthinking-tools": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-sequentialthinking-tools"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "gossiphs-mcp-server": {
      "command": "uvx",
      "args": [
        "gossiphs-mcp",
        "server"
      ]
    },
    "code-parser": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/boxabirds/mcp-code-parser",
        "mcp-code-parser",
        "serve"
      ]
    }
  },
  "features": {
    "checkpointing": {
      "enabled": true
    }
  },
  "autoAccept": true,
  "usageStatisticsEnabled": false,
  "contextFileName": "CLAUDE.md",
}
