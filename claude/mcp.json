{
  "mcpServers": {
    "sequential-thinking-tools": {
      "command": "npx",
      "args": ["-y", "mcp-sequentialthinking-tools"]
    },
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git"]
    },
    "github-cli": {
      "command": "npx",
      "args": ["-y", "gh-cli-mcp"]
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "code-parser": {
      "command": "uvx",
      "args": [
          "--from",
          "git+https://github.com/boxabirds/mcp-code-parser",
          "mcp-code-parser",
          "serve"
      ]
    }
  }
}
